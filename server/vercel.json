{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "crons": [
    {
      "path": "/api/cron",

      "schedule": "14 15 7 22 8 *"
    }
  ]
}
